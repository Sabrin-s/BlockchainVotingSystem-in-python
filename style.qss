QPushButton {
    padding: 10px;
    font-size: 14px;
    background-color: #4CAF50;
    color: white;
    border-radius: 5px;
}
QPushButton:hover {
    background-color: #45a049;
}
QLineEdit, QComboBox {
    padding: 8px;
    font-size: 14px;
    border: 1px solid #ccc;
    border-radius: 4px;
    margin-bottom: 10px;
}
